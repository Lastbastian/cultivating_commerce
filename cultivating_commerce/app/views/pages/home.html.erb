<div class="container-fluid debugger">

  <header class="debugger">
    <div class="nav-bar">
      <h1 class="logo">Cultivated Commerce</h1>
      <div class="nav-right">
        <h3>User Name</h3>
          <%= link_to 'Sign out', destroy_user_session_path, :method => :delete %>    
      </div>
      <div>
        <img class="user-img-s" src="">
      </div>
    </div>
  </header>

  <section class="map col-md-5 debugger">
    <div>
      <a href="">Add location</a>
    </div>
    <div class="googlemap">
      <iframe
      src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d50457.15380196072!2d-122.36215482530719!3d37.77663006075536!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x80859a6d00690021%3A0x4a501367f076adff!2sSan+Francisco%2C+CA!5e0!3m2!1sen!2sus!4v1417302995695" width="100%" height="450" frameborder="0" style="border:0">
      </iframe>
    </div>
  </section>

  <div class="col-md-7 debugger"> <!-- position right part -->
    <section class="filters debugger">
      <div class="filter-days">
        <ul class="days">
          <li class="day">gathering-day</li>
        </ul>
      </div>
      <div class="filter-type">
        <ul class="type-icons">
          <li class="type-icon">gathering-type</li>
        </ul>
      </div>
      <div class="filter-search">
        <%= form_tag('/', :method => "get", id: "search-form", class:"search-form") do %>
          <%= text_field_tag :search, params[:search], placeholder: "Search Listings" %>
          <%= submit_tag "Search", :name => nil %>
        <% end %>
      </div>    
    </section>

    <section class="listings-section debugger">
      <div><a href="">Add a product</a></div>
        <ul class="listings">
          <% if @listings && !@listings.empty? %>   
            <% @listings.each do |l| %>     
              <%= render 'listing', l: l %>
            <% end %>
          <% end %>         
        </ul>
      </div>
    </section>

  </div> <!-- end of right container-->
</div> <!-- end of container -->