<p>My details</p>
<% if user_signed_in? %>
  <p>Welcome <%= current_user.email %></p>
  <%= link_to 'Logged In [click to logout]', destroy_user_session_path, :method => :delete %>
<% else %>
  <p>You are not signed in.</p>
  <%= link_to 'Login', new_user_session_path %>
<% end %>

<% if @current_user %>
  <% current_user.events.each do |e| %>
    <%= e.title %>
  <% end %>
<% end %>


